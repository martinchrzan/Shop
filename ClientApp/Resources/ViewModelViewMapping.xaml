<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ClientApp.Resources"
                    xmlns:localViewModels="clr-namespace:ClientApp.ViewModels"
                    xmlns:localViews="clr-namespace:ClientApp.Views">

    <DataTemplate DataType="{x:Type localViewModels:BasketViewModel}">
        <localViews:BasketView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type localViewModels:ProductItemViewModel}">
        <localViews:ProductItemView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type localViewModels:UserViewModel}">
        <localViews:UserView/>
    </DataTemplate>
</ResourceDictionary>